{
    "name": "arjunphp/laravel-fullcalendar",
    "description": "Laravel helper for FullCalendar.io",
    "license": "MIT",
    "authors": [
        {
            "name": "Shawn Tunney",
            "email": "shawn.tunney@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.3",
        "illuminate/support": "^5|^6|^7|^8|^9"
    },
    "autoload": {
        "psr-0": {
            "Arjunphp\\LaravelFullcalendar": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Arjunphp\\LaravelFullcalendar\\ServiceProvider"
            ],
            "aliases": {
                "Calendar": "Arjunphp\\LaravelFullcalendar\\Facades\\Calendar"
            }
        }
    },
    "minimum-stability": "stable"
}
